void on_clicked_Label1(GtkWidget *widget, gpointer data) {
    GtkLabel *label = (GtkLabel*) data;
    gtk_label_set_text(label, "START");
    g_print("START Button Clicked\n");
}
void on_clicked_Label2(GtkWidget *widget, gpointer data) {
    GtkLabel *label = (GtkLabel*) data;
    gtk_label_set_text(label, "Instrucciones");
    g_print("Instrucciones Button Clicked\n");
}

